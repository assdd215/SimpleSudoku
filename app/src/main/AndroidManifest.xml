<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aria.sudokuweb">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SelectLevelAty" />
        <activity
            android:name=".activity.RuleAty"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        <activity android:name=".activity.GameAty" />
        <activity
            android:name=".activity.SelectNumberAty"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        <activity android:name=".activity.RankAty" />
        <activity android:name=".activity.InputMsgAty"></activity>
    </application>

</manifest>